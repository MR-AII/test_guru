<h1>Create New Questions</h1>

<form action="/tests/1/questions/create" method="post">
  <input name="authenticity_token" value="<%= form_authenticity_token %>" type="hidden">

  <p>
    <label for="question-title">Title:</label>
    <input type="text" name="question[title]" id="question-title">
  </p><br>

  <p>
    <label for="question-body">Body:</label>
    <input type="text" name="question[body]" id="question-body">
  </p><br>

  <input type="submit" value="Create">
</form>