<p>
  <% @test.questions.each do |question| %>
  <p>
    <strong>Title:</strong>
    <%= question.title %>
  </p>

  <p>
    <strong>Body:</strong>
    <%= question.body %>
  </p>
  <%= link_to 'Show', test_questions_path(@test, question) %>
  <%= link_to 'Edit', edit_test_question_path(@test, question) %>
  <%= link_to 'Delete', test_questions_path(question),
              method: :delete,
              data: { confirm: 'Are you sure?' } %>
<% end %>
</p>