<tr>
  <td><%= test.title %></td>
  <td><%= test_level(test) %></td>
  <td><%= test.questions.count %> </td>
  <% if current_user.is_a?(Admin) %>
    <td><%= current_user.first_name %></td>
  <% end %>
  <td><%= link_to 'Show', admin_test_path(test) %></td>
  <td><%= link_to 'Edit', edit_admin_test_path(test) %></td>
  <td><%= link_to 'Delete', admin_test_path(test),
                  method: :delete,
                  data: { confirm: 'Are you sure?' } %></td>
</tr>